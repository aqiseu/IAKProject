            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Scrolling2Activity"
            android:label="@string/title_activity_scrolling2"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Scrolling3Activity"
            android:label="@string/title_activity_scrolling3"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Scrolling4Activity"
            android:label="@string/title_activity_scrolling4"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Scrolling5Activity"
            android:label="@string/title_activity_scrolling5"
            android:theme="@style/AppTheme.NoActionBar">

        </activity>
    </application>

</manifest>